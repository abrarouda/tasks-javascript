




{{>header}}
<section class="container">
<table class="table my-5">
  <thead class="thead-dark">
    <tr>
      <th scope="col">First name</th>
      <th scope="col">Last name</th>
      <th scope="col">Address</th>
      <th scope="col">Birthday</th>
      <th scope="col">Operation</th>
    </tr>
  </thead>
  <tbody>
      
        <tr>
            <td>{{privateData.firstName}}{{result.firstName}}</td>
            <td>{{privateData.lastName}}{{result.lastName}}</td>
            <td>{{privateData.address}}{{result.address}}</td>
            <td>{{privateData.birthday}}{{result.birthday}}</td>
            <td>
                <a href="/edit/{{privateData._id}}{{result._id}}" class="btn btn-primary" >edit</a>
                <a href="/delete/{{privateData._id}}{{result._id}}" class="btn btn-danger" >delete</a>
            </td>
            
        </tr>
     
  </tbody>
</table>

</section>
{{>footer}}
